# Connect docker

docker run -it -d --name telua_gtest your_image_name tail -f /dev/null
docker ps -a
docker exec -it telua_gtest bash

cat /var/log/auth.log
cat /etc/ssh/sshd_config


service ssh status
service ssh restart


# start docker

docker compose up -d

# Rebuild
docker compose down
docker compose up --build -d


# ssh docker
ssh root@localhost -p 2222

scp  -P 2222 ReadMe  root@localhost:/opt
